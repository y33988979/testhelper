DVD_MAKE_VERSION := 2

## 公共变量与规则
include $(DVD_MAKE)/generic.mak

## 编译选项

## 模块名
LIBNAME := yscas

## 输入库名
ST40_IMPORTS :=

## 输出库名
ST40_EXPORTS := lib$(LIBNAME).a

## 对象文件列表
OBJS = drv_cas.o

## 目标库名
ST40_TARGETS := lib$(LIBNAME).a


## 缺省变量与规则
include $(DVD_MAKE)/defrules.mak


## 创建目标库
# -------------
$(ST40_TARGETS) : $(ST40_IMPORTS) $(OBJS)
	@$(ECHO) Building $@
	$(BUILD_LIBRARY)

## 清除模块
.PHONY : clean
clean:
	@$(ECHO) ---- Cleaning yscas ----
	-$(RM) *.o
	-$(RM) *.a

# 依赖关系
drv_cas.tco: drv_cas.c

